{
  "name": "vue-awesome-template",
  "version": "1.4.0",
  "description": "A Vue.js project template",
  "author": "BryanAdamss@foxmail.com",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "cross-env BUILD_MODE=dev webpack-dev-server  --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "unit:watch": "jest --config test/unit/jest.conf.js --watchAll",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "build": "cross-env BUILD_MODE=prod node build/build.js",
    "build:test": "cross-env BUILD_MODE=test node build/build.js",
    "build:measure": "cross-env MEASURE=true npm run build",
    "dll": "webpack --config build/webpack.dll.conf.js",
    "release": "standard-version",
    "release:patch": "npm run release -r patch",
    "release:minor": "npm run release -r minor",
    "release:major": "npm run release -r major",
    "release:first": "npm run release -- --first-release",
    "release:publish": "git push --follow-tags origin master",
    "commit": "git-cz",
    "mock": "node mock/index.js",
    "stylelint": "stylelint src/**/*.vue",
    "contributor:init": "all-contributors init",
    "contributor:add": "all-contributors add",
    "contributor:generate": "all-contributors generate"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "better-scroll": "^1.13.4",
    "element-ui": "^2.8.2",
    "velocity-animate": "^1.5.2",
    "vue": "^2.6.10",
    "vue-lazyload": "^1.2.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "all-contributors-cli": "^6.13.0",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^2.0.1",
    "chromedriver": "^2.46.0",
    "commitizen": "^4.0.3",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.2.0",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.0",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-include-assets-plugin": "^1.0.6",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^0.3.0",
    "launch-editor-middleware": "^2.2.1",
    "lint-staged": "^9.4.2",
    "mocker-api": "^1.9.0",
    "nightwatch": "^0.9.12",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-url": "^7.2.1",
    "preload-webpack-plugin": "^2.3.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "selenium-server": "^3.0.1",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "speed-measure-webpack-plugin": "^1.3.1",
    "standard-version": "^7.0.0",
    "stylelint": "^12.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^3.1.1",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.2.2",
    "svgo-loader": "^2.2.1",
    "thread-loader": "^2.1.3",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "url-loader": "^0.5.8",
    "vconsole": "^3.2.0",
    "vue-jest": "^1.0.2",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 8.12.0",
    "npm": ">= 5.12.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,scss,less,sass,md,html,flow,ts,tsd}": [
      "prettier --write",
      "git add"
    ]
  }
}
